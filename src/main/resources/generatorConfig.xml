<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <!-- JDBCを指定 -->
  <classPathEntry location="C:\Users\usr160056\.m2\repository\mysql\mysql-connector-java\5.1.25\mysql-connector-java-5.1.25.jar" />

  <context id="MysqlTables" targetRuntime="MyBatis3">

    <commentGenerator>
      <property name="suppressDate" value="true" />
    </commentGenerator>

    <jdbcConnection
        driverClass="org.gjt.mm.mysql.Driver" 
        connectionURL="jdbc:mysql://127.0.0.1:3306/mygmor_template_test"
        userId="unittest"
        password="unittest">
    </jdbcConnection>

    <javaTypeResolver>
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.tt.sabamiso.domain" targetProject="sabamiso">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
      <property name="rootClass" value="com.tt.sabamiso.common.domain.BaseDomain" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.tt.sabamiso.integration" targetProject="sabamiso\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.tt.sabamiso.integration" targetProject="sabamiso">
      <property name="enableSubPackages" value="true" />
      <property name="rootInterface" value="com.tt.sabamiso.common.integration.Mapper" />
    </javaClientGenerator>

    <!-- 対象テーブルを指定 -->
    <table tableName="users" domainObjectName="User"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
            selectByPrimaryKeyQueryId="false"
            selectByExampleQueryId="false">
      <property name="useActualColumnNames" value="false" />
    </table>

  </context>
</generatorConfiguration>
